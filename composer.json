{
    "name": "spectero/marketplace",
    "description": "Proprietary Spectero Marketplace",
    "license": "Restrictive",
    "type": "project",
    "require": {
        "php": ">=7.2",
        "ext-intl" : "*",
        "laravel/lumen-framework": "5.5.*",
        "vlucas/phpdotenv": "~2.2",
        "flipbox/lumen-generator": "^5.4",
        "dusterio/lumen-passport": "^0.2.4",
        "guzzlehttp/guzzle": "~6.0",
        "silber/bouncer": "dev-master",
        "pragmarx/google2fa": "^2.0",
        "illuminate/redis": "^5.5",
        "srmklive/paypal": "^1.5",
        "illuminate/mail": "^5.5",
        "nordsoftware/lumen-cors": "^2.2",
        "cartalyst/stripe-laravel": "7.0.*",
        "geoip2/geoip2": "~2.0",
        "lcobucci/jwt": "^3.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "laravel/homestead": "^7.0",
        "barryvdh/laravel-ide-helper": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-update-cmd": [
            "php artisan ide-helper:generate"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    }
}
